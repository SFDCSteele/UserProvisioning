<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>T2H_Fiscal_Year__c</fullName>
    <description>The fiscal year of the New Hire Actual Start Date on which this onboard will be counted for T2H.</description>
    <externalId>false</externalId>
    <formula>IF(ISNULL(New_Hire_Actual_Start__c), null, &quot;FY&quot;&amp;Text(YEAR(New_Hire_Actual_Start__c)+1-IF(MONTH(New_Hire_Actual_Start__c)&lt;10,1,0)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>T2H Fiscal Year</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
