<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FEP_Disable_Approval_of_Cancelled_record</fullName>
    <active>true</active>
    <description>Prevents users from approving cancelled records by mistake.</description>
    <errorConditionFormula>AND(
ISPICKVAL(PRIORVALUE(Approval_Status__c), &apos;Cancelled&apos;),
ISPICKVAL(Approval_Status__c, &apos;Approved&apos;),
 RecordType.Name == &quot;FAST&quot;
)</errorConditionFormula>
    <errorMessage>You cannot approve a cancelled application access record.</errorMessage>
</ValidationRule>
